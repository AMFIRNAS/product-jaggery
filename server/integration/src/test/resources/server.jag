<%

var log = new Log();
websocket.ontext = function(data) {
    log.info('Client Sent : ' + data);
    var ws = this;
    setTimeout(function() {
        ws.send(data);
    }, 100);
};

websocket.onopen = function(data) {
    print('Open WS');
};

websocket.onclose = function(data) {
    print('Close WS')
};

websocket.onbinary = function(stream) {
    log.info('Client Streamed : ' + stream.toString());
}; 

%>